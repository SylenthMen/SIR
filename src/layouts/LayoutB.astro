---
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
  </head>
  <body class="bg-black">
    <Navbar />
    <div class="felx">
      <div>
        <a class="text-white float-left ml-12 hover:text-[#ff9f33]" href="#"
          >ANTERIOR</a
        >
      </div>
      <div>
        <a class="text-white float-right mr-12 hover:text-[#ff9f33]" href="#"
          >SIGUIENTE</a
        >
      </div>
    </div>
    <slot />
    <Footer />
  </body>
</html>
